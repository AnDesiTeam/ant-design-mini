<view class="ant-calendar">
  <block
    a:for="{{ todoList }}"
    a:for-index="taskIndex"
    a:for-item="task"
    a:key="{{ task.id }}">
    <!-- display: inline -->
    <text data-item-id="{{ taskIndex }}"
      >{{ taskIndex }}{{ task.content }}</text
    >
  </block>
  <test loading />
</view>
