<form-input
  label="用户{{index + 1}}"
  name="account{{index}}"
  required
  message="需要输入用户名"
  ref="handleRef"
  a:for="{{list}}"
>
  <view slot="extra" onTap="minus" data-index="{{index}}">
    <icon type="MinusCircleOutline" />
  </view>
</form-input>

<button onTap="add">
  添加用户
  <icon type="AddCircleOutline" />
</button>

<view style="margin-top: 20px">
  <button type="primary" onTap="submit">1提交</button>
  <button onTap="reset">重置</button>
</view>
