<view class="ant-calendar">
  <block
    wx:for="{{ todoList }}"
    wx:for-index="taskIndex"
    wx:for-item="task"
    wx:key="{{ task.id }}">
    <!-- display: inline -->
    <text
      hidden="{{ !mixin.value }}"
      data-item-id="{{ taskIndex }}"
      data-num="{{ 20 }}"
      >{{ taskIndex }}{{ task }}</text
    >
  </block>
  <test loading />
</view>
