<view>
  <scroll-view
    scroll-y
    class="amd-indexbar-scroll"
    scroll-into-view="{{scrollId}}"
    onScroll="onScroll">
    <view a:for="{{items}}" class="amd-indexbar-list" id="{{`amd-indexbar-${item}`}}">
      <list header="{{item}}">
        <list-item a:for="{{4}}" a:for-item="itemX">
          {{item}}-{{itemX}}
        </list-item>
      </list>
    </view>
  </scroll-view>
  <view class="amd-indexbar-right">
    <indexbar
      items="{{items}}"
      defaultCurrent="A"
      current="{{current}}"
      onChange="onChange"/>
  </view>
</view>