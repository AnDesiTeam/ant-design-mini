<form-input
  label="用户名"
  name="username"
  ref="handleRef"
/>

<form-image-upload
  label="图片上传"
  name="imageList"
  onUpload="onUpload"
  maxCount="{{3}}"
  tooltip="需要上传3张图片"
  ref="handleRef"
/>

<view class="buttons">
  <button type="primary" onTap="submit" style="margin-bottom: 12px;">提交</button>
  <button onTap="reset">重置</button>
</view>
