<container title="受控模式">
  <uploader
    fileList="{{fileList}}"
    onChange="onChange"
    onUpload="onUpload" />
</container>

<container title="手动触发上传">
  <uploader
    ref="handleUploaderRef"
    fileList="{{fileList}}"
    onChange="onChange"
    onUpload="onUpload" />
  <button
    size="default"
    onTap="upload"
    type="primary">
    上传
  </button>
</container>
