<!--basic-->
<tabs
  items="{{items}}"
  current="{{current}}"
  onChange="onChange"
/>

<view style="margin-bottom: 12px;">
  {{items[current].content}}
</view>


<!--capsule-->
<tabs
  type="capsule"
  items="{{items}}"
  current="{{current}}"
  onChange="onChange"
/>

<view style="margin-bottom: 12px;">
  {{items[current].content}}
</view>


<!--mixin-->
<tabs
  type="mixin"
  items="{{items}}"
  current="{{current}}"
  onChange="onChange"
/>

<view>
  {{items[current].content}}
</view>
