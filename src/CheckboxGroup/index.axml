<list className="amd-checkbox-group {{className ? className : ''}}" radius="{{radius}}">
  <view class="amd-list-header">
    <slot name="header">{{footer}}</slot>
  </view>
  <checkbox-group id="{{id ? id : $id}}" name="{{name ? name : ''}}" disabled="{{_disabled}}">
    <view class="amd-list-body">
      <slot />
    </view>
  </checkbox-group>
  <view class="amd-list-footer">
    <slot name="footer">{{footer}}</slot>
  </view>
</list>