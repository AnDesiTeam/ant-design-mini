<form-input
  label="用户名"
  name="account"
  tooltip="用户账户对应名称"
  allowClear
  required
  message="需要填写用户名"
  ref="handleRef"
/>

<form-input
  label="地址"
  name="address"
  allowClear
  ref="handleRef"
/>

<form-switch
  label="选择"
  name="choose"
  ref="handleRef"
/>

<button type="primary" onTap="submit">提交</button>
<button onTap="reset">重置</button>
