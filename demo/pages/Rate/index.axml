
<container title="默认用法">
    <rate allowClear="{{true}}" value="{{rate1}}" onChange="handleChange" />
</container>

<container title="半星">
    <rate value="{{4.5}}" allowHalf="{{true}}" allowClear="{{true}}" onChange="handleChange" />
</container>

<container title="禁用">
    <rate value="{{4.5}}" disabled="{{true}}" />
    <rate className="rate" value="{{4}}" disabled="{{true}}" />
</container>

<container title="只读">
    <rate value="{{4.5}}" readOnly="{{true}}" />
    <rate className="rate" value="{{4}}" readOnly="{{true}}" />
</container>

<container title="自定义">
