<view class="amd-footer-tag {{className ? className : ''}}" style="{{style || ''}}">
  <block a:for="{{tags}}">
    <view 
      class="amd-footer-tag-text {{onTap ? 'amd-footer-tag-text-click': ''}}"
      data-index="{{index}}"
      onTap="onTap"
    >
      {{item}}
    </view>
  </block>
</view>
