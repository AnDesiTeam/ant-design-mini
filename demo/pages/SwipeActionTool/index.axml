        onTouchEnd="onTouchEnd">
        <view class="t-swipe-item-con">
          {{ item.detail }}
        </view>
      </swipe-action-tool>
  </view>
  <view class="t-swipe-title">滑动二次确认</view>
  <view class="t-swipe-item" a:for="{{baseRightList2}}">
