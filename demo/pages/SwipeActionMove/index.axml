<view class="t-swipe">
  <view class="t-swipe-item">
    <view class="t-swipe-item-title">删除按钮滑动到最大距离触发二次确认</view>
    <swipe-action
      a:for="{{ 10 }}"
      data-item="{{{ index }}}"
      rightButtons="{{ rightBtns }}"
      elasticity="{{true}}"
      swiped="{{ swipeIndex === index }}"
      onSwipeEnd="onSwipeEnd"
      onSwipeStart="onSwipeStart"
      >
      <view class="t-swipe-item-con">
        右侧-三个按钮
      </view>
    </swipe-action>
  </view>
</view>